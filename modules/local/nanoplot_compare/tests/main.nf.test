nextflow_process {

    name "Test Process NANOPLOT_COMPARE"
    script "../main.nf"
    process "NANOPLOT_COMPARE"

    tag "modules"
    tag "modules_local"
    tag "nanoplot_compare"

    test("nanoplot_compare - stub run") {

        options "-stub"

        when {
            process {
                """
                input[0] = []
                input[1] = 'test_comparison'
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

}
